
# //////////////////////////////////////////////////////////////////////////// #
# // Speed Spread Configuration file ///////////////////////////////////////// #
# //////////////////////////////////////////////////////////////////////////// #


TARGET_HOST  ?= default_selected_target
TARGET_PORT  ?= default_port
TARGET_SPOOL ?= deafault_spool_dir

# List of the parallel channels to be used in connection
channels = 1 2 4

# Segment size [KB]
segments = 64

# Number of samples to collect per segment size
samples = 50

# Plot limits for tcp/udt throughput histograms axis [min,max] [MBps]
speed_limits_tcp = auto
speed_limits_udt = auto

